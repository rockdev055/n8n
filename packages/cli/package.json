        "flatted": "^2.0.0",
        "glob-promise": "^3.4.0",
        "google-timezones-json": "^1.0.2",
        "localtunnel": "^1.9.1",
        "mongodb": "^3.2.3",
        "n8n-core": "^0.1.0",
        "n8n-editor-ui": "^0.1.0",
        "n8n-nodes-base": "^0.2.1",
        "n8n-workflow": "^0.1.0",
        "open": "^6.1.0",
