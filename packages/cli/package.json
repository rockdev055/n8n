    "files": [
        "dist"
    ],
    "devDependencies": {
        "@types/config": "0.0.34",
        "@types/connect-history-api-fallback": "^1.3.1",
        "@types/express": "^4.16.1",
        "@types/jest": "^23.3.2",
        "@types/localtunnel": "^1.9.0",
        "@types/node": "^10.10.1",
        "@types/open": "^6.1.0",
        "@types/parseurl": "^1.3.1",
        "@types/request-promise-native": "^1.0.15",
        "@types/vorpal": "^1.11.0",
        "jest": "^23.6.0",
        "nodemon": "^1.19.1",
        "sails-disk": "^1.0.1",
        "ts-jest": "^23.10.1",
        "tslint": "^5.11.0",
        "typescript": "~3.5.2"
    },
    "dependencies": {
        "body-parser": "^1.18.3",
        "config": "^3.0.1",
